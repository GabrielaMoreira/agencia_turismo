<p id="notice"><%= notice %></p>

<h1>Fundadores</h1>

<table>
  <thead>
    <tr>
      <th>Data nasc</th>
      <th>Data morte</th>
      <th>Nome</th>
      <th>Nacionalidade</th>
      <th>Atividade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fundadores.each do |fundador| %>
      <tr>
        <td><%= fundador.data_nasc %></td>
        <td><%= fundador.data_morte %></td>
        <td><%= fundador.nome %></td>
        <td><%= fundador.nacionalidade %></td>
        <td><%= fundador.atividade %></td>
        <td><%= link_to 'Show', fundador %></td>
        <td><%= link_to 'Edit', edit_fundador_path(fundador) %></td>
        <td><%= link_to 'Destroy', fundador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fundador', new_fundador_path %>
